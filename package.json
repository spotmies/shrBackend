    "build": "^0.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "gitignore": "^0.7.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.1",
