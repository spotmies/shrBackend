    "build": "tsc --outDir dist",
    "start": "node dist/server.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source/typeorm.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source/typeorm.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/data-source/typeorm.ts",
    "setup:bucket": "npx ts-node src/scripts/createSupabaseBucket.ts",
    "setup:uploads-bucket": "npx ts-node src/scripts/createUploadsBucket.ts"
  },
